<div id="{{ctx.groupId}}" class="panel-group builder-sidebar{{ctx.scrollEnabled ? ' builder-sidebar_scroll' : ''}}" ref="sidebar">
  <input class="form-control builder-sidebar_search" type="text" ref="sidebar-search" placeholder="Search field(s)" />
  <div ref="sidebar-groups">
    {% ctx.groups.forEach(function(group) { %}
      {{ group }}
    {% }) %}
  </div>
</div>
